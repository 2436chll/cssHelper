{
  "name": "auto-completion-suggestion",
  "displayName": "auto-completion-suggestion",
  "description": "提供常用中文提示自动补全。",
  "version": "0.0.2",
  "publisher": "luckyLi",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:html",
    "onLanguage:vue"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "auto-completion-suggestion",
      "properties": {
        "auto-completion-suggestion.aapaas-theme.enable": {
          "type": "boolean",
          "default": true,
          "description": "开启/关闭 aapaas样式属性 中文提示补全"
        },
        "auto-completion-suggestion.a-css-helper.enable": {
          "type": "boolean",
          "default": true,
          "description": "开启/关闭 a-css class 中文提示补全"
        }
      }
    }
  },
  "scripts": {
    "package": "vsce package"
  },
  "devDependencies": {
    "vsce": "^2.15.0"
  }
}